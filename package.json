{
    "name": "code-save-slots",
    "displayName": "Code Save Slots",
    "description": "Save slots for your code editing.",
    "version": "0.0.1",
    "publisher": "micnil",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:saveSlots"
    ],
    "main": "./out/extension",
    "contributes": {
        "views": {
			"explorer": [
				{
					"id": "saveSlots",
					"name": "Save Slots"
                }
            ]
        },
        "commands": [
            {
                "command": "saveSlots.quickSave",
                "title": "Quick Save"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
